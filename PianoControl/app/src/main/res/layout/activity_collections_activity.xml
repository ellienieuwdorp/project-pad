<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.pad.sss04.pianocontrol.CollectionsActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/btnCow"
        android:layout_width="wrap_content"
        android:layout_height="92dp"
        android:text="Cow"
        app:layout_constraintRight_toLeftOf="@+id/btnBurps"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="51dp"
        app:layout_constraintTop_toBottomOf="@+id/btnFarts" />

    <Button
        android:id="@+id/btnFarts"
        android:layout_width="wrap_content"
        android:layout_height="92dp"
        android:text="Farts "
        tools:layout_editor_absoluteY="78dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/btnPiano" />

    <Button
        android:id="@+id/btnSheep"
        android:layout_width="wrap_content"
        android:layout_height="93dp"
        android:text="Sheep"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="60dp"
        app:layout_constraintTop_toBottomOf="@+id/btnCow"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnPiano"
        android:layout_width="wrap_content"
        android:layout_height="93dp"
        android:text="Piano"
        tools:layout_editor_absoluteY="78dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btnFarts" />

    <Button
        android:id="@+id/btnBurps"
        android:layout_width="wrap_content"
        android:layout_height="93dp"
        android:text="Burps"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btnCow"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@+id/btnPiano" />
</android.support.constraint.ConstraintLayout>
